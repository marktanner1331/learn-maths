    "@roxi/routify": "^2.18.18",
    "@tsconfig/svelte": "^3.0.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^4.0.1",
    "rollup-plugin-svelte": "^7.1.2",
    "sass": "^1.86.3",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.0"
